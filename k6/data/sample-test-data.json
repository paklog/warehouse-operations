  "testScenarios": {
    "peakHours": {
      "description": "Simulate peak warehouse operations",
      "timeRange": "09:00-11:00, 14:00-16:00",
      "loadMultiplier": 2.5
    },
    "nightShift": {
      "description": "Simulate reduced night operations", 
      "timeRange": "22:00-06:00",
      "loadMultiplier": 0.3
    },
    "holidayRush": {
      "description": "Simulate holiday season load",
      "timeRange": "all_day",
      "loadMultiplier": 4.0
    },
    "systemMaintenance": {
      "description": "Simulate load during maintenance windows",
      "timeRange": "02:00-04:00",
      "loadMultiplier": 0.1
    }
  },
  "validationRules": {
    "packageValidation": {
      "maxItemsPerPackage": 50,
      "maxWeightKg": 22,
      "requiredFields": ["orderType", "items", "shippingAddress"]
    },
    "pickListValidation": {
      "maxPicksPerList": 25,
      "requiredFields": ["orderId", "pickerId", "instructions"]
    },
    "pickConfirmation": {
      "requiredFields": ["skuCode", "quantity", "binLocation"],
      "maxQuantityDeviation": 0.1
    }
  }
